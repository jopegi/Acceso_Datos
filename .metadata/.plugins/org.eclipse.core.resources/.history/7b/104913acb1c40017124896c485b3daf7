import org.jsoup.Jsoup;
import org.jsoup.nodes.Document;
import org.jsoup.select.Elements;

public class Parser2 {
	
	//Definimos la url de la página web cuyo contenido queremnos parsear
	public static final String url = "https://www.pccomponentes.com";

	public static void main(String[] args) {
		
        // Obtengo el HTML de la web en un objeto Document
        Document doc = Jsoup.parse(url);
		
      //Elements entradas = document.select("div.col-xs-6 col-sm-4 col-md-3 col-lg-3 article-block").not("tarjeta-articulo__elementos-basicos");
        Elements entradas = doc.select("div.tarjeta-articulo__extras"); 
        
        System.out.println(entradas.size());

	}

}
